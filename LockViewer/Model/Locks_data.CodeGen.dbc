			begin
				disposable data reader	,@BinaryReader	,new BinaryReader(stream)
				tmpSTRUCT.csGuid = reader.ReadString()
				tmpString = reader.ReadString()
				tmpSTRUCT.csProcess_id = Convert.ToInt32(tmpString)
				tmpSTRUCT.csTimestamp = reader.ReadString()
				tmpSTRUCT.csFile_operation = reader.ReadString()
				tmpSTRUCT.csRecord_key = reader.ReadString()
				tmpSTRUCT.csFile_name = reader.ReadString()
				tmpString = reader.ReadString()
				tmpSTRUCT.csLock_status = Convert.ToBoolean(tmpString)
				tmpString = reader.ReadString()
				if (tmpString.Length > 0) tmpSTRUCT.csLock_date = Convert.ToDateTime(tmpString)
				tmpString = reader.ReadString()
				tmpSTRUCT.csLock_time = Convert.ToInt32(tmpString)
				tmpSTRUCT.csUsername = reader.ReadString()
				tmpSTRUCT.csModule = reader.ReadString()
			end

