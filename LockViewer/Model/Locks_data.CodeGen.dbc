		private static method structureToByteArray	,[#]byte
			tmpSTRUCT				,clsstrLocks
			endparams
		proc
			data stream	,@MemoryStream , new MemoryStream()
			begin
				disposable data writer ,@BinaryWriter , new BinaryWriter(stream)
				writer.Write(tmpSTRUCT.csGuid)
				writer.Write(tmpSTRUCT.csProcess_id.ToString())
				writer.Write(tmpSTRUCT.csTimestamp)
				writer.Write(tmpSTRUCT.csFile_operation)
				writer.Write(tmpSTRUCT.csRecord_key)
				writer.Write(tmpSTRUCT.csFile_name)
				writer.Write(tmpSTRUCT.csLock_status.ToString())
				if (tmpSTRUCT.csLock_date.Ticks >= DateTime.MinValue.Ticks && tmpSTRUCT.csLock_date.Ticks <= DateTime.MaxValue.Ticks) then
					writer.Write(tmpSTRUCT.csLock_date.ToString("yyyy'-'MM'-'dd'T'HH':'mm':'ss'.'fff'Z'"))
				else
					writer.Write("")
				writer.Write(tmpSTRUCT.csLock_time.ToString())
				writer.Write(tmpSTRUCT.csUsername)
				writer.Write(tmpSTRUCT.csModule)
			end
			mreturn stream.ToArray()
		endmethod
		
		private static method byteArrayToStructure	,clsstrLocks
